{
  "version": 2,
  "builds": [
    {
      "src": "bot.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "src/**",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "bot.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "src/$1"
    }
  ]
}
